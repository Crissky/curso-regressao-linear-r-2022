dir <- c(102, 101, 94, 79, 79)
esq <- c(175, 169, 182, 146, 144)
## Reta de regressao -----
mod = lm(esq~dir)
mod
summary(mod)
## Visualizando o ajuste do modelo graficamente -----
plot(esq ~ dir, xlab = "bra?o direito", ylab = "bra?o esquerdo", main = "Pressao (mmHg)")
abline(mod, col = "red")
### Primeira forma -----
# pressao.esq = 43.565 + 1.315*pressao.dir
pressao.esq = 43.565 + 1.315*100
pressao.esq  # 175.065
### Segunda forma -----
# fun??o predict()
predict(lm(esq~dir)) # retorna os valores de y, para os valores de x existentes
