trinta.d <- c(244, 260, 264, 264, 278, 318, 280)
um.d <- c(456, 614, 567, 943, 628, 1088, 536)
## Reta de regress?o ----
mod2 = lm(um.d ~ trinta.d)
mod2
summary(mod2)
## Visualizando o ajuste do modelo graficamente -----
plot(um.d ~ trinta.d)
abline(mod2, col = "red")
### Primeira forma ------
# pass.um.dia = -1237.626 + 7.073*pass.trinta.dias
pass.um.dia = -1237.626 + 7.073*300
pass.um.dia # 884.274
### Segunda forma ----
predict(lm(um.d ~ trinta.d))
pass.trinta.d <- data.frame(trinta.d = c(300))
predict(lm(um.d ~ trinta.d), pass.trinta.d)  # 884.2894
#Dados
co2 <- c(314, 317, 320, 326, 331, 339, 346, 354, 361, 369)
temp <- c(13.9, 14, 13.9, 14.1, 14, 14.3, 14.1, 14.5, 14.5, 14.4)
## Reta de regressao -----
mod3 = lm(temp ~ co2)
mod3
## Visualizando o ajuste do modelo graficamente -----
plot(temp ~ co2)
abline(mod3, col = "red")
### Primeira forma -----
# temperatura = 10.48308 + 0.01092*concentracao de CO2
temperatura = 10.48308 + 0.01092*370.9
temperatura # 14.53331
### Segunda forma -------
predict(mod3)
concent <- data.frame(co2 = 370.9)
predict(mod3, concent)  # 14.53247
#Dados
cric <- c(882, 1188, 1104, 864, 1200, 1032, 960, 900)
temper <- c(69.7, 93.3, 84.3, 76.3, 88.6, 82.6, 71.6, 79.6)
## Reta de regressao ------
mod4 = lm(temper ~ cric)
mod4
## Visualizando o ajuste do modelo graficamente ----
plot(temper ~ cric)
abline(mod4, col = "red")
### Primeira forma -----
# temperaturaF = 27.62835 + 0.05227*cricrilos em 1 minuto
temperaturaF = 27.62835 + 0.05227*3000
temperaturaF # 184.4384
### Segunda forma ----
predict(mod4)
# Encontrando valores de y, para valores de x novos:
cric.min <- data.frame(cric = 3000)
predict(mod4, cric.min)  # 184.445
#Dados
atriz <- c(26, 80, 42, 29, 33, 35, 45, 49, 39, 34, 26, 25, 33, 35, 35, 28, 20, 29, 61)
ator <- c(51, 32, 42, 54, 52, 37, 38, 32, 45, 60, 46, 40, 36, 47, 29, 43, 37, 38, 45)
## Reta de regressao -----
mod5 = lm(ator ~ atriz)
mod5
plot(ator ~ atriz)
abline(mod5, col = "red")
### Primeira forma -----
# idade.ator = 49.0625 - 0.1821*idade.atriz
idade.ator = 49.0625 - 0.1821*75
idade.ator # 35
## Descobrindo idades de atores (y) para novas idades das atrizes (x):
atrizes <- data.frame(atriz = c(75))
predict(lm(ator ~ atriz), atrizes)  # 35
